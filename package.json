{
  "name": "tevel-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run backend:run\"",
    "build": "tsc -b && vite build && npm run backend:build",
    "lint": "eslint .",
    "preview": "vite preview",
    "backend:build": "cargo build --manifest-path=backend/Cargo.toml",
    "backend:run": "cargo run --manifest-path=backend/Cargo.toml -- --ws-addr 0.0.0.0:${WS_PORT:-8080} --in-addr ${IN_SERVER_IP:-127.0.0.1}:${IN_SERVER_PORT:-9001} --out-addr ${OUT_SERVER_IP:-127.0.0.1}:${OUT_SERVER_PORT:-9002}"
  },
  "dependencies": {
    "@types/pegjs": "^0.10.6",
    "peggy": "^4.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-use-websocket": "^4.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0",
    "concurrently": "^8.2.2"
  }
}
