struct Person {
  type: i32,
  height: f64
}

struct Employee {
  id: i64,
  l: T(i8),
  l2: T(i8),
  arr: [T(i8); id],
  person: match l : T{
    Lol => Person,
    V => match l2 : T {
      Lol => i8,
      V => i32,
    }
  }
}

struct Main {
  employee: Employee,
  person: Person,
  status: T(i8)
}

enum T {
  Lol = 1,
  V = 2
} 
