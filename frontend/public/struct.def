struct Person {
  type: i32,
  height: i64
}

struct Employee {
  id: i64,
  l: T(i8),
  l2: T(i8),
  arr: [T(i8); id],
  s: CString,
  person: match l: T{
    Lol => Person,
    H => Person,
    V => match l2: T {
      Lol => i8,
      H => i8,
      V => i32,
    }
  }
}

struct Main {
  employee: Employee,
  person: Person,
  status: T(i8)
}

enum T {
  Lol = 0,
  H = 1,
  V = 2
} 
